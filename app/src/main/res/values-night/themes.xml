<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProjecBinarKBG" parent="Theme.AppCompat.DayNight">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/light_blue_A200</item>
        <item name="colorPrimaryVariant">@color/light_blue_600</item>
        <item name="colorOnPrimary">#202020</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#202020</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.FullScreen" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:statusBarColor" tools:targetApi="l">#202020</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>
    </style>

    <style name="Theme.SplashScreen" parent="Theme.FullScreen" />
</resources>